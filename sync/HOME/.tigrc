#
# bindings
#
bind generic <F1> view-help
bind generic q view-close
bind generic j move-down
bind generic k move-up
bind generic <C-F> scroll-page-down
bind generic <C-B> scroll-page-up
bind generic <Space> scroll-page-down
bind generic b scroll-page-up
bind generic g move-first-line
bind generic G move-last-line
bind main G move-last-line
bind generic / search
bind generic ? search-back
bind generic <Enter> enter
bind generic <Space>s view-status
bind generic <Space>r view-refs
bind generic f ?git fetch %(remote)
bind refs C ?git checkout %(branch)
bind generic ! ?git reset --hard %(commit)

